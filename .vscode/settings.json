{
  "emmet.includeLanguages": {
    "postcss": "css"
  },
  "tailwindCSS.classAttributes": [
    ".*[Cc]lasses",
    ".*[Cc]lass[Nn]ame"
  ],
  "tailwindCSS.experimental.classRegex": [
    [
      "\\S*[Cc]lasses\\S* = \\[([\\s\\S]+?)(\\]\\n|\\n\\])",
      "'([^']*)'"
    ],
    [
      "\\S*[Cc]lasses\\S* = \\[([\\s\\S]+?)(\\]\\n|\\n\\])",
      "\"([^\"]*)\""
    ],
    [
      "\\S*[Cc]lasses\\S* = \\[([\\s\\S]+?)(\\]\\n|\\n\\])",
      "`([^`]*)`"
    ],
    [
      "(\\S*[Cc]lasses: \\[[\\s\\S]*\\])",
      "'([^']*)'"
    ],
    [
      "(\\S*[Cc]lasses: \\[[\\s\\S]*\\])",
      "\"([^\"]*)\""
    ],
    [
      "(\\S*[Cc]lasses: \\[[\\s\\S]*\\])",
      "`([^`]*)`"
    ],
    [
      "(\\S*[Cc]lasses: '[\\s\\S]*'+)",
      "'([^']*)'"
    ],
    [
      "(\\S*[Cc]lasses: \"[\\s\\S]*\"+)",
      "\"([^\"]*)\""
    ],
    [
      "(\\S*[Cc]lasses: `[\\s\\S]*`+)",
      "`([^`]*)`"
    ],
    [
      "(\\S*[Cc]lass[Nn]ame: '[\\s\\S]*'+)",
      "'([^']*)'"
    ],
    [
      "(\\S*[Cc]lass[Nn]ame: \"[\\s\\S]*\"+)",
      "\"([^\"]*)\""
    ],
    [
      "(\\S*[Cc]lass[Nn]ame: `[\\s\\S]*`+)",
      "`([^`]*)`"
    ]
    // ["classcat\\(([^]*)\\)", "(?:'|\"|`)([^\"'`]*)(?:'|\"|`)"]
  ],
  "cSpell.words": [
    "clsx"
  ],
}